{
  "name": "eleventy-plugin-postcss",
  "version": "1.0.4",
  "description": "Eleventy plugin for PostCSS",
  "main": ".eleventy.js",
  "engines": {
    "node": ">=7.6.0"
  },
  "11ty": {
    "compatibility": ">=1.0.0"
  },
  "scripts": {
    "demo": "npx @11ty/eleventy --input=demo --output=demo/_site --config=demo/.eleventy.js",
    "demo:cleanup": "rm -rf ./demo/_site",
    "start": "npx @11ty/eleventy --input=demo --output=demo/_site --config=demo/.eleventy.js --serve",
    "lint": "eslint .",
    "test": "ava",
    "pretest": "npm run demo",
    "posttest": "npm run demo:cleanup"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pewstiepoll/eleventy-plugin-postcss.git"
  },
  "keywords": [
    "eleventy",
    "11ty",
    "postcss",
    "post",
    "css"
  ],
  "author": "Vadym (https://github.com/pewstiepoll)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pewstiepoll/eleventy-plugin-postcss/issues"
  },
  "homepage": "https://github.com/pewstiepoll/eleventy-plugin-postcss#readme",
  "dependencies": {
    "postcss": "8.4.31",
    "postcss-load-config": "3.1.3"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0",
    "ava": "4.1.0",
    "eslint": "8.11.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "2.25.4",
    "postcss-mixins": "9.0.2",
    "postcss-preset-env": "7.4.2"
  }
}
